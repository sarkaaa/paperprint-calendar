    "next": "14.1.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^18",
    "react-calendar": "^4.8.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "react-scroll": "^1.9.0",
    "react-to-print": "^2.15.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.3",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "lint-staged": "^15.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.ts": "eslint --fix ",
    "*.{ts,js,json}": "prettier --cache --write"
