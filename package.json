{
  "name": "paperprint-calendar",
  "version": "0.1.0",
  "private": true,
  "author": "Sarka Chwastkova",
  "description": "Print your custom calendar!",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
